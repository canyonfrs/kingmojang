{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "kingmojang",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.kingmojang",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": ["https://github.com/canyonfrs/kingmojang/releases"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDMzQ0M5MjYzMjNEMzg3MkMKUldRc2g5TWpZNUxNTXdTT1FRRVFxY0tWSkFOcnZUSWhRR09wYUc5ZmlGMEE4Y3k5T0lrbC9KMkgK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "킹모장",
        "width": 800,
        "height": 600
      }
    ]
  }
}
